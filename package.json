{
  "name": "tvcountdown",
  "private": true,
  "author": "Thomas Hermann <novascreen@gmail.com>",
  "license": "MIT",
  "proxy": "http://localhost:4000",
  "scripts": {
    "start": "concurrently \"npm:start:*\"",
    "start:app": "craco start",
    "start:server": "ts-node-dev -P server/tsconfig.json -r tsconfig-paths/register --inspect -T --ignore-watch node_modules server/api.dev.ts",
    "build": "npm-run-all -s build:*",
    "build:client": "craco build",
    "build:server": "cd server && ncc build index.ts -o ../api",
    "now-build": "npm-run-all -s build:*",
    "test": "craco test",
    "eject": "react-scripts eject",
    "gen-types": "graphql-codegen"
  },
  "dependencies": {
    "@material-ui/core": "^4.6.0",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/styles": "^4.6.0",
    "@types/jest": "^24.0.22",
    "@types/moment": "^2.13.0",
    "@types/node": "^13.9.7",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@zeit/ncc": "^0.20.5",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link-context": "^1.0.19",
    "apollo-link-http": "^1.5.16",
    "apollo-link-state": "^0.4.2",
    "apollo-server-micro": "^2.9.7",
    "auth0-js": "^9.11.3",
    "express-jwt": "^5.3.1",
    "graphql": "^14.5.8",
    "graphql-import": "^0.7.1",
    "history": "^4.10.1",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^1.6.0",
    "lodash": "^4.17.15",
    "memoizee": "^0.4.14",
    "micro": "^9.3.5-canary.3",
    "microrouter": "^3.1.3",
    "moment": "^2.24.0",
    "node-fetch": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "prisma-client-lib": "^1.34.10",
    "ramda": "^0.26.1",
    "react": "^16.11.0",
    "react-adopt": "^0.6.0",
    "react-apollo": "^3.1.3",
    "react-autosuggest": "^9.4.3",
    "react-dom": "^16.11.0",
    "react-infinite": "^0.13.0",
    "react-intl": "^3.4.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "recompose": "^0.30.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "3.7.2",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.4",
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@craco/craco": "^5.6.1",
    "@graphql-codegen/cli": "^1.8.3",
    "@graphql-codegen/typescript": "^1.8.3",
    "@graphql-codegen/typescript-operations": "^1.8.3",
    "@graphql-codegen/typescript-resolvers": "^1.8.3",
    "@types/auth0-js": "^9.10.6",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/lodash": "^4.14.146",
    "@types/memoizee": "^0.4.3",
    "@types/microrouter": "^3.1.1",
    "@types/node-fetch": "^2.5.3",
    "@types/query-string": "^6.3.0",
    "@types/ramda": "^0.26.33",
    "@types/react-autosuggest": "^9.3.13",
    "@types/react-infinite": "^0.0.34",
    "@types/react-router": "^5.1.2",
    "@types/react-router-dom": "^5.1.2",
    "@types/recompose": "^0.30.7",
    "@types/uuid": "^3.4.6",
    "concurrently": "^5.0.0",
    "craco-workbox": "^0.1.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "prisma": "^1.34.10",
    "tsconfig-paths": "^3.9.0"
  },
  "engines": {
    "node": ">= 10.16.0"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "no-restricted-globals": "off",
      "@typescript-eslint/no-use-before-define": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
