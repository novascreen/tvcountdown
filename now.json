{
  "version": 2,
  "name": "tvcountdown",
  "alias": "tvcountdown.info",
  "build": {
    "env": {
      "REACT_APP_AUTH_DOMAIN": "@tvcountdown-auth-domain",
      "REACT_APP_AUTH_CLIENT_ID": "@tvcountdown-auth-client-id",
      "REACT_APP_AUTH_REDIRECT_URI": "@tvcountdown-auth-redirect-uri",
      "REACT_APP_AUTH_DB_CONNECTION_NAME": "Username-Password-Authentication",
      "REACT_APP_AUTH_AUDIENCE": "@tvcountdown-auth-audience"
    }
  },
  "env": {
    "PRISMA_STAGE": "prod",
    "PRISMA_ENDPOINT": "@tvcountdown-prisma-endpoint",
    "PRISMA_SECRET": "@tvcountdown-prisma-secret",
    "APP_SECRET": "@tvcountdown-app-secret",
    "AUTH_DOMAIN": "@tvcountdown-auth-domain",
    "AUTH_AUDIENCE": "@tvcountdown-auth-audience"
  }
}
